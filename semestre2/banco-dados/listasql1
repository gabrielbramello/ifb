CREATE TABLE BANCO(
    CODIGO INTEGER PRIMARY KEY,
    NOME VARCHAR(50)
    );

    CREATE TABLE AGENCIA (
	NUM_AGEN VARCHAR(4),
	ENDERECO VARCHAR(50),
	COD_BANCO VARCHAR(1),
	PRIMARY KEY(COD_BANCO, NUM_AGEN)
    );
CREATE TABLE CLIENTE (
    CPF INTEGER,
    NOME VARCHAR(50),
    SEXO TEXT,
    ENDERECO VARCHAR(50)
    );
CREATE TABLE CONTA (
    NUMERO_CONTA INTEGER PRIMARY KEY,
    SALDO FLOAT,
    TIPO_CONTA TEXT,
    NUM_AGENCIA INTEGER
    );
CREATE TABLE HISTORICO (
    CPF_CLIENTE INTEGER,
    NUM_CONTA INTEGER,
    DATA_INICIO VARCHAR(50)
    );
    CREATE TABLE TELEFONE_CLIENTE (
    CPF_CLIENTE INTEGER,
    TELEFONE INTEGER
    );
