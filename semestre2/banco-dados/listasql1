ALTER TABLE AGENCIA
ADD CONSTRAINT FK8 FOREIGN KEY (cod_banco) REFERENCES BANCO(CODIGO);

ALTER TABLE cliente
ALTER COLUMN cpf TYPE VARCHAR(15);

ALTER TABLE CONTA
ALTER COLUMN numero_conta TYPE VARCHAR(7);

ALTER TABLE HISTORICO
ALTER COLUMN cpf_cliente TYPE VARCHAR(15);

ALTER TABLE HISTORICO
ALTER COLUMN num_conta TYPE VARCHAR(7);

ALTER TABLE TELEFONE_CLIENTE
ALTER COLUMN cpf_cliente TYPE VARCHAR(15);

